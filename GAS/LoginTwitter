//Twitterのアクセストークン
var twitter = TwitterWebService.getInstance(
  '【API Key】',
  '【API secret key】'
);

//アプリを連携認証する
function Authorize() {
  twitter.authorize();
}

//認証を解除する
function Reset() {
  twitter.reset();
}

//認証後のコールバック
function authCallback(request) {
  return twitter.authCallback(request);
}
